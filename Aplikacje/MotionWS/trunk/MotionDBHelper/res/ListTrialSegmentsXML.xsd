<?xml version="1.0" encoding="utf-8"?>
<s:schema id="ListTrialSegmentsXML" xmlns:s="http://www.w3.org/2001/XMLSchema"  elementFormDefault="qualified">
  <s:element name="TrailSegmentList">
    <s:complexType>
      <s:sequence>
        <s:element name="SegmentDetails" minOccurs="0" maxOccurs="unbounded">
          <s:complexType>
            <s:attribute name="SegmentID" type="s:int" />
            <s:attribute name="TrialID" type="s:int" />
            <s:attribute name="SegmentName" type="s:string" />
            <s:attribute name="StartTime" type="s:int" />
            <s:attribute name="EndTime" type="s:int" />
          </s:complexType>
        </s:element>
      </s:sequence>
    </s:complexType>
  </s:element>
</s:schema>
